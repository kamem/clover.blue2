{
  "name": "clover.blue",
  "host": "https://clover.blue",
  "version": "1.0.0",
  "description": "とあるフロントエンドの技術日記",
  "author": "kamem",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js --watch server",
    "dev:mock": "axios-mock-server -b && cross-env NODE_ENV=development MOCK=true nodemon --exec babel-node server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server_dist/index.js",
    "generate": "nuxt generate",
    "build:server": "babel server/ -d server_dist",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "diff": "node test/diff/"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11",
    "Android >= 4"
  ],
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/feed": "^1.1.0",
    "@nuxtjs/google-tag-manager": "^2.2.1",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/robots": "^2.4.0",
    "@nuxtjs/sitemap": "^1.3.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "autoprefixer": "^9.6.1",
    "axios-mock-server": "^0.10.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "express": "^4.16.4",
    "firebase-admin": "^8.4.0",
    "husky": "^2.6.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.2.1",
    "marked": "^0.7.0",
    "moment": "^2.24.0",
    "nodemon": "^1.18.9",
    "nuxt": "^2.0.0",
    "nuxt-jsonld": "^1.4.2",
    "postcss-each": "^0.10.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-extend": "^1.0.5",
    "postcss-for": "^2.1.1",
    "postcss-foreach": "^0.0.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.16.4",
    "resemblejs": "^3.2.2",
    "vue-jest": "^4.0.0-0",
    "vue-scrollto": "^2.17.1",
    "vue-youtube": "^1.3.5",
    "vue2-touch-events": "^2.0.0"
  }
}
